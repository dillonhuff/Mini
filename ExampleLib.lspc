operation matrixAdd(r matrix double 32 32 1 32 a,
	  	    r matrix double 32 32 1 32 b,
		    output matrix double 32 32 1 32 c) {
  c = a + b;
}

operation strangeOp(r matrix float 12 1 1 1 x,
	  	    rw matrix float 12 1 1 1 y,
		    r matrix float 12 1 1 1 z,
		    output matrix float 12 1 1 1 k) {
  z = z + x + y;
  k = z - k;
}