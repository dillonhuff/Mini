operation dgemm(r matrix double 1 1 1 1 alpha,
	  	r matrix double 1 1 1 1 beta,
		r matrix double gen gen gen gen A,
		r matrix double gen gen gen gen B,
		output matrix double gen gen gen gen C) {
  C = alpha .* A*B + beta .* C;
}


operation sgemm(r matrix float 1 1 1 1 alpha,
	  	r matrix float 1 1 1 1 beta,
		r matrix float gen gen gen gen A,
		r matrix float gen gen gen gen B,
		output matrix float gen gen gen gen C) {
  C = alpha .* A*B + beta .* C;
}